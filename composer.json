{
    "name": "iikiti/mfa",
    "description": "Two-factor/multi-factor authentication bundle for Symfony and iikiti.",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "iikiti\\MfaBundle\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "iikiti\\MfaBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.18",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.8",
        "friendsofphp/php-cs-fixer": "*",
        "psalm/plugin-symfony": "^5.1",
        "psalm/attributes": "*"
    },
    "require": {
		"php": "~8.2.0 || ~8.3.0",
        "symfony/security-bundle": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/options-resolver": "^7.0",
        "spomky-labs/otphp": "^11.2",
        "doctrine/persistence": "^3.2",
        "symfony/orm-pack": "^2.4",
        "symfony/property-access": "^7.0"
    },
	"suggest": {
        "ext-sodium": "Base32 encoding at higher performance."
    }
}
