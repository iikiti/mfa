{
    "name": "iikiti/mfa",
    "description": "Two-factor/multi-factor authentication bundle for Symfony and iikiti.",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "iikiti\\MfaBundle\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "iikiti\\MfaBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.24",
        "squizlabs/php_codesniffer": "^3.10",
        "friendsofphp/php-cs-fixer": "*",
        "psalm/plugin-symfony": "^5.1",
        "psalm/attributes": "*",
        "symfony/test-pack": "^1.1"
    },
    "require": {
		"php": "~8.2.0 || ~8.3.0",
        "symfony/security-bundle": "^7.1",
        "symfony/routing": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/validator": "^7.1",
        "symfony/options-resolver": "^7.1",
        "spomky-labs/otphp": "^11.2",
        "doctrine/persistence": "^3.3",
        "symfony/property-access": "^7.1"
    },
	"suggest": {
        "ext-sodium": "Base32 encoding at higher performance."
    }
}
